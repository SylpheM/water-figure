    <force x="59" y="19" x2="63" y2="30" valueX="0.3" valueY="1.0"/>
    <force x="70" y="45" x2="75" y2="60" valueX="0.1" valueY="1.0"/>
    <force x="67" y="70" x2="72" y2="75" valueX="-0.3" valueY="-0.4"/>
    <force x="53" y="87" x2="57" y2="89" valueX="-0.8" valueY="0.0"/>
